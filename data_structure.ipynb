    "n = int(input())\n",
    "\n",
    "queue = [x for x in range(1, n+1)]\n",
    "\n",
    "while len(queue)>1:\n",
    "    queue.pop(0)\n",
    "    queue.append(queue.pop(0))\n",
    "    \n",
    "print(queue[0])"
